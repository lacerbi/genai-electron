{
  "name": "genai-electron",
  "version": "0.1.0",
  "description": "Electron-specific library for managing local AI model servers and resources",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "rm -rf dist coverage",
    "prepublishOnly": "npm run clean && npm run build && npm test"
  },
  "keywords": [
    "electron",
    "ai",
    "llm",
    "llama.cpp",
    "stable-diffusion",
    "local-ai",
    "gguf",
    "machine-learning",
    "genai"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/genai-electron.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/genai-electron/issues"
  },
  "homepage": "https://github.com/yourusername/genai-electron#readme",
  "peerDependencies": {
    "electron": ">=25.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.10.2",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^30.0.0-alpha.6",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.2"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
}
